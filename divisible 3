using System;

namespace FirstApp
{
    class Program
    {
        static void Main(string[] args)
        {
            int conter = 0;
            for (int i = 1; i <= 100; i++)
            {
                if (i % 3 == 0)
                {
                    conter++;
                }
            }
            Console.WriteLine(conter);

        }
    }

   
    


    
}
