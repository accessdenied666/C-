using System;

namespace FirstApp
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.Write("Enter a number : "); 
            int x = int.Parse(Console.ReadLine());
            if (x>0 && x<11)  
            {
                Console.WriteLine("Valid");
                   

            }
            else
            {
                Console.WriteLine("Invalid");
            }
            

            

        }
    }

   
    
